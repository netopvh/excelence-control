{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": "off",
        "no-useless-constructor": "off",
        "no-prototype-builtins": "off",
        "no-useless-escape": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-new": "off"
    }
}